<?xml version="1.0" encoding="UTF-8"?>
<!--
	The IBM DataStage specific template configuration.
	TODO: Not sure yet what is allowed in DataStage identifiers.
	A DataStage package stores the description of nodes in a child 'Property' element where @Name='Description'.
	The Job element doesn't seem to have a Description, so this needs to be done in the config.
-->
<FileFormat
	type="IBM_DataStage"
	version="*"
	templateType="xml"
	currentAccessor="_"
	childAccessor="$"
	commentNodeXPath="Property[@Name=&quot;Description&quot;]"
	annotationPrefix="@XGen"
	annotationArgsPrefix="("
	annotationArgsSuffix=")"
/>