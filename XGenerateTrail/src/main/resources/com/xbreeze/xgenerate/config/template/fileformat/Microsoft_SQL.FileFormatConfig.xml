<?xml version="1.0" encoding="UTF-8"?>
<!--
	The Microsoft SQL specific template configuration.
	In T-SQL the identify fields allowed are @, $, # & _. We use _ for the current and $ for child.
	The prefix of an annotation. In T-SQL a comment starts with a double hyphen and the annotation starts with an @XGen.
-->
<FileFormat
	type="Microsoft_SQL"
	version="*"
	templateType="text"
	currentAccessor="_"
	childAccessor="$"
	commentPrefix="--"
	annotationPrefix="@XGen"
	annotationArgsPrefix="("
	annotationArgsSuffix=")"
/>