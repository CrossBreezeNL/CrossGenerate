<?xml version="1.0" encoding="UTF-8"?>
<!--
	The ANSI SQL specific template configuration.
	In ANSI SQL the special character allowed in identifier is an underscore (_). We use one for the current and two underscores for child.
	The prefix of an annotation. In ANSI SQL a comment starts with a double hyphen and the annotation starts with an @XGen.
	https://www.w3schools.com/sql/sql_comments.asp
-->
<FileFormat
	type="ANSI_SQL"
	version="*"
	templateType="text"
	currentAccessor="_"
	childAccessor="__"
	commentPrefix="--"
	annotationPrefix="@XGen"
	annotationArgsPrefix="("
	annotationArgsSuffix=")"
/>